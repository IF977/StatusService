<%= render 'header' %>
<div id="page-wrapper">
	<div class="container-fluid">
		<div class="row">
			<div class="col-lg-12">
				<h1 class="page-header">Empresas </h1> 
				<table> 
					<thead> 
						<tr> 
							<th>Nome</th> 
							<th></th> 
						</tr> 
					</thead> 
					<tbody> 
						<% @companies.each do |company| %> 
						<tr> 
							<td><%= company.nome %></td>
						</tr> 
						<%end%> 
					</tbody> 
				</table>
			</div>
		</div>
	</div>
</div>